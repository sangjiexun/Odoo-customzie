<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!--Product No-->
        <record id="sequence_template_product_no" model="ir.sequence">
            <field name="name">Product No</field>
            <field name="code">product.template</field>
            <field name="prefix">PT</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <!--field name="company_id" eval="False"/-->
        </record>

        <record id="sequence_product_product_no" model="ir.sequence">
            <field name="name">Product No</field>
            <field name="code">product.product</field>
            <field name="prefix">PD</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <!--field name="company_id" eval="False"/-->
        </record>

    </data>
</odoo>
