<?xml version="1.0"?>
<odoo>
    <record id="print_product_barcode_wizard" model="ir.ui.view">
        <field name="name">Product Barcode Print Wizard</field>
        <field name="model">momo.barcode.print.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <from>
                <group>
                    <field name="start_row"/>
                    <field name="start_column"/>
                </group>
                <footer>
                    <button string="Cancel" special="cancel"/>
                    <button name="print_product_barcode" string="Start Print" type="object" class="oe_highlight"/>
                </footer>
            </from>
        </field>
    </record>

        <record id="print_user_barcode_wizard" model="ir.ui.view">
        <field name="name">User Barcode Print Wizard</field>
        <field name="model">momo.barcode.print.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <from>
                <group>
                    <field name="start_row"/>
                    <field name="start_column"/>
                </group>
                <footer>
                    <button string="Cancel" special="cancel"/>
                    <button name="print_user_barcode" string="Start Print" type="object" class="oe_highlight"/>
                </footer>
            </from>
        </field>
    </record>

</odoo>